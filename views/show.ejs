<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Pesquisa de satisfação</title>
</head>
<body bgcolor="#DCDCDC">
  

<h2>Registro de participantes</h2>



  <table border="1">
    <thead>
        <tr>
            <td>Nome</td>
            <td>Sobrenome</td>
            <td>Genero</td>
            <td>Data de Nascimento</td>
            <td>Idade</td>
            <td>Email</td>
            <td>Endereco</td>
            <td>Telefone</td>
            <td>Profissão</td>
            <td>Medidas</td>
            <td>Investimento</td>
            <td>Prevenção</td>
            <td>Apoio</td>
            <td>Postura</td>
            

            <td>Ação</td>

        </tr>
        
    </thead>
    <tbody>
    
    <% data.forEach(function(details) { %>
        <tr>
            <td><%= details.name %></td>
            <td><%= details.surname %></td>
            <td><%= details.gender %></td>
            <td><%= details.datanascimento %> </td>
            <td><%= details.idade %></td>
            <td><%= details.email %></td>
            <td><%= details.endereco %></td>
            <td><%= details.telefone %></td>
            <td><%= details.profissao %></td>
            <td><%= details.medidas %></td>
            <td><%= details.investimento %></td>
            <td><%= details.prevencao %></td>
            <td><%= details.apoio %></td>
            <td><%= details.postura %></td>
            
            <td><a href="/edit/<%= details._id %>">Editar</a> - <a href="/delete/<%= details._id %>">Deletar</a></td>
        </tr>
        <% }) %>
    </tbody>
  </table>
  <button><a href="/">Novo</a></button>
 

  
</body>
</html